{% extends 'base.html.twig' %}

{% block title %}Commande index{% endblock %}

{% block body %}
    <!-- start navigation -->
	{% include ('navbaradmin.html.twig') %}
	<!-- this generates the error -->

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DateCommande</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for commande in commandes %}
            <tr>
                <td>{{ commande.id }}</td>
                <td>{{ commande.dateCommande ? commande.dateCommande|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('crud_commande_show', {'id': commande.id}) }}">show</a>
                    <a href="{{ path('crud_commande_edit', {'id': commande.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
