{% extends 'base.html.twig' %}

{% block title %}Commande index{% endblock %}

{% block body %}
    <!-- start navigation -->
	{% include ('navbaradmin.html.twig') %}
	<!-- this generates the error -->

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DateCommande</th>
                <th>IdProduit</th>
                <th>IdUser</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for commande in commandes %}
            <tr>
                <td>{{ commande.id }}</td>
                <td>{{ commande.dateCommande ? commande.dateCommande|date('Y-m-d') : '' }}</td>
                <td>{{ commande.idProduit }}</td>
                <td>{{ commande.idUser }}</td>
                 <td>
                    {{ include('crud_commande/_delete_form.html.twig') }}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
